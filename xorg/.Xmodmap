keycode  10 = 1 exclam eacute
keycode  11 = 2 at egrave
keycode  12 = 3 numbersign ecircumflex
keycode  13 = 4 dollar icircumflex
keycode  14 = 5 percent ugrave idiaeresis
keycode  15 = 6 asciicircum ucircumflex udiaeresis
keycode  16 = 7 ampersand ocircumflex odiaeresis
keycode  17 = 8 asterisk aacute
keycode  18 = 9 parenleft agrave
keycode  19 = 0 parenright acircumflex ediaeresis
keycode  24 = q Q adiaeresis
keycode  26 = e E EuroSign cent
keycode  32 = o O oe OE
keycode  38 = a A ae AE
keycode  54 = c C ccedilla

add mod3 = Mode_switch
add mod4 = Multi_key
