# "-*- sh -*-"

function mv()
{
    command mv -iv $@
}

# TODO replace with a function
# problem: zmv is *already* a function
# so it cause a recursive call that
# cannot be avoided using `builtin' or `command'
alias zmv="zmv -M"

function mvi()
{
    mv $1 `dirname $1`/$2
}

