# "-*- sh -*-"

function nsw()
{
    if [ $# -ne 0 ]; then
	for i; do
	    ns_who | grep "$i"
	done
    else
	ns_who
    fi
}

function nshw()
{
    if [ $# -ne 0 ]; then
	for i; do
	    ns_hwho | grep "$i"
	done
    else
	ns_hwho
    fi
}

alias ns="${HOME}/local/bin/soulmebaby&; sleep 15; kdestroy"
