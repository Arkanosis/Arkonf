# "-*- sh -*-"

typeset -U PATH

export PATH=$HOME/local_$arch/bin/:$HOME/local/bin/:$PATH
export LD_LIBRARY_PATH=$HOME/local_$arch/lib/:$HOME/local/lib/:$HOME/local/lib64/:$HOME/local/lib32/:$PATH
export C_INCLUDE_PATH=$HOME/local_$arch/include/:$HOME/local/include/:$PATH
export CPLUS_INCLUDE_PATH=$C_INCLUDE_PATH
export PYTHONPATH=$HOME/local/lib/python:$HOME/local/lib/python2.4/site-packages:$PYTHONPATH/
export CONFIG_SITE=$HOME/config.site
