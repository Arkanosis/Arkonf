# "-*- sh -*-"

function rmbrute()
{
    for arg; do
	if [[ -n $arg && -e $arg ]]; then
	    find $arg -exec chmod u+rw {} \;
	    rm -rf $arg
	fi
    done
}

function clean()
{
    rm -f *.o(N)
    rm -f *.aux(N)
    rm -f *.log(N)
    rm -f *.toc(N)
    rm -f *~(N)
    rm -f .*~(N)
    rm -f \#*\#(N)
}
